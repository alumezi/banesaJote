{
    "name": "banesajote",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "engines": {
        "node": "14.15.1",
        "npm": "6.14.8"
    },
    "scripts": {
        "build": "tsc",
        "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
        "prestart": "npm run build",
        "start": "NODE_ENV=production node dist/js/index.js",
        "start:dev": "cross-env NODE_ENV=development concurrently \"tsc -w\" \"nodemon dist/js/index.js\"",
        "test": "cross-env NODE_ENV=test jest --verbose --runInBand",
        "build:ui": "rm -rf build && cd ./client && npm run build --prod  && cp -r build ../dist/js",
        "deploy:manual": "git push heroku main",
        "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push && git push heroku",
        "deploy:only": "git add . && git commit -m deploy && git push heroku",
        "logs:prod": "heroku logs --tail",
        "lint": "eslint ."
    },
    "author": "Arbnor Lumezi",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "chalk": "^4.1.1",
        "concurrently": "^6.2.1",
        "cookie-parser": "^1.4.5",
        "cookie-session": "^1.4.0",
        "cors": "^2.8.5",
        "cross-env": "^7.0.2",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "express-session": "^1.17.2",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.9.21",
        "mongoose-unique-validator": "^2.0.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.3",
        "nodemailer": "^6.6.2",
        "passport": "^0.4.1",
        "passport-facebook": "^3.0.0",
        "passport-local": "^1.0.0",
        "short-uuid": "^4.2.0",
        "supertest": "^4.0.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-session": "^2.0.43",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/mongodb": "^4.0.7",
        "@types/mongoose": "^5.11.97",
        "@types/mongoose-unique-validator": "^1.0.5",
